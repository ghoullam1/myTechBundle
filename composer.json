{
    "name":"sniper/tech-bundle",
    "description":"Tech Bundle",
    "type":"symfony-bundle",
    "license":"M.I.T",
    "authors":[
        {
            "name":"GHALLAM Abdelghani",
            "email":"ghalam.autec@gmail.com"
        }
    ],
    "require":{
        "php":"~7.1.3",
        "lexik/jwt-authentication-bundle":"~2.6.5",
        "knplabs/knp-paginator-bundle":"^2.8",
        "symfony/orm-pack":"*",
        "twig/twig":"~2.0",
        "symfony/form":"*",
        "symfony/http-kernel":"*",
        "symfony/dependency-injection":"^4.3",
        "doctrine/annotations":"~1.8",
        "symfony/config":"*",
        "symfony/translation":"*",
        "symfony/yaml":"*"
    },
    "require-dev":{
        "phing/phing":"~2.16.1",
        "phpunit/phpunit":"~7.3.5",
        "php-mock/php-mock-phpunit":"^2.1",
        "squizlabs/php_codesniffer":"~3.3.2",
        "escapestudios/symfony2-coding-standard":"~3.4.1",
        "php-http/mock-client":"^1.1",
        "doctrine/doctrine-fixtures-bundle":"^3.0",
        "fzaninotto/faker":"^1.9",
        "symfony/debug-pack":"*",
        "symfony/maker-bundle":"^1.0",
        "symfony/profiler-pack":"*",
        "symfony/test-pack":"*"
    },
    "autoload":{
        "psr-4":{
            "Sniper\\Bundle\\TechBundle\\":""
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.3.*"
        }
    }
}

